{
  "name": "digitoimistodude/so-clean-up-wp-seo",
  "description": "Hide most of the bloat that the Yoast SEO plugin adds to your WordPress Dashboard",
  "type": "wordpress-plugin",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "SO WP",
      "homepage": "https://github.com/digitoimistodude/so-clean-up-wp-seo"
    }
  ],
  "require": {
    "php": ">=5.6",
    "composer/installers": "^1.0 || ^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7 || ^6.5 || ^7.5",
    "wp-coding-standards/wpcs": "^2.3",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.7.0"
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "installer-name": "so-clean-up-wp-seo"
  }
}